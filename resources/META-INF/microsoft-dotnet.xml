<idea-plugin>
	<extensions defaultExtensionNs="com.intellij">
		<moduleExtensionProvider key="microsoft-ikvm"
								 parentKey="microsoft-dotnet"
								 name="Java (IKVM.NET)"
								 icon="/icons/java.png"
								 immutableClass="org.mustbe.consulo.ikvm.microsoft.module.extension.MicrosoftIkvmModuleExtension"
								 mutableClass="org.mustbe.consulo.ikvm.microsoft.module.extension.MicrosoftIkvmMutableModuleExtension"/>
	</extensions>

	<extensions defaultExtensionNs="org.mustbe.consulo.dotnet.core">
		<dependencyCopierExtension implementation="org.mustbe.consulo.ikvm.microsoft.compiler.IkvmDependencyCopierExtension" />
	</extensions>
</idea-plugin>